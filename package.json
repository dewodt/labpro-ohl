{
  "name": "labpro-ohl",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run build:client:prod && npm run build:tailwind:prod",
    "start": "nest start",
    "start:prod": "node dist/src/main",
    "build:tailwind:prod": "tailwindcss -i assets/styles/tailwind.css -o public/styles/tailwind.css --minify",
    "build:client:prod": "tsc --project tsconfig.client.json && npm run minify:client",
    "minify:client": "for f in ./public/scripts/*.js; do terser $f -o $f --compress --mangle; done",
    "seed": "node dist/src/seeder",
    "migration:generate:prod": "env-cmd -f .env.production ./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:generate -d src/database/ormconfig.ts",
    "migration:up:prod": "env-cmd -f .env.production ./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run -d src/database/ormconfig.ts",
    "migration:down:prod": "env-cmd -f .env.production ./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:revert -d src/database/ormconfig.ts",
    "migration:create:prod": "env-cmd -f .env.production ./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:create",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --tsc --watch",
    "build:tailwind:dev": "tailwindcss -i assets/styles/tailwind.css -o public/styles/tailwind.css --watch",
    "build:client:dev": "tsc --project tsconfig.client.json --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" && eslint --fix \"src/**/*.ts\" \"test/**/*.ts\" && echo \"\\e[0;32mâœ”\\e[0;0m Linted files\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.4.0",
    "cookie-parser": "^1.4.6",
    "hbs": "^4.2.0",
    "nestjs-form-data": "^1.9.91",
    "nestjs-pino": "^4.1.0",
    "pg": "^8.12.0",
    "pino-http": "^10.2.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "tailwindcss-animate": "^1.0.7",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/hbs": "^4.0.4",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.20",
    "env-cmd": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.1.4",
    "install": "^0.13.0",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.9",
    "npm": "^10.8.2",
    "pino-pretty": "^11.2.2",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.10",
    "terser": "^5.31.6",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.17.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
